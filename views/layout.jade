doctype html
html
  head
    link(rel='stylesheet' href="https://bootswatch.com/superhero/bootstrap.css")
    script(src="//code.jquery.com/jquery-1.12.0.min.js")
    title #{title}
    link(rel='stylesheet', href='/stylesheets/style.css')
  header
    div(class='navbar navbar-default')
      div(class='container-fluid')
        div(class='navbar-header')
          img(src='/images/LogoWhite.png' class='logo')
        div(class='appName')
          h1 Note Catcher

  body
    block content
    
  footer
    .container.footer
      div(class='footerSection')
        img(src='/images/GrowthGroupsLogo.png' class='footerIcons')
        p(class='footerText') Growth Groups 
      div(class='footerSection')
        img(src='/images/ChildrensLogo.png' class='footerIcons')
        p(class='footerText') Children's Ministries
      div(class='footerSection')
        img(src='/images/StudentLogo.png' class='footerIcons')
        p(class='footerText') Student Ministries
      div(class='footerSection')
        img(src='/images/MissionsLogo.png' class='footerIcons')
        p(class='footerText') Missions
      div(class='footerSection')
        img(src='/images/CareLogo.png' class='footerIcons')
        p(class='footerText') Care Ministries
      div(class='footerSection')
        img(src='/images/WorshipLogo.png' class='footerIcons')
        p(class='footerText') Worship
  
